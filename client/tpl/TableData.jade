h2.text-info Table Data: #{table.name}
div(id='TableData-#{table.id}-Sheet')
    .row-fluid
        .span1
            strong Column
        .span11.row
            for column in columnList.list 
                .span3
                    span(id='TableData-Header-Column-#{column.id}-Name') 
                        strong #{column.name}
                    span(id='TableData-Header-Column-#{column.id}-Type') (#{column.type})
    .row-fluid
        .span1
            #TableData-Index.row-fluid
        .span11.row
            #TableData-Data.row-fluid
