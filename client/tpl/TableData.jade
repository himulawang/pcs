h2#TableData-Header.text-info Table Data: #{table.name}
div(id='TableData-#{table.id}-Sheet')
    #TableData-Title
        .TableData-Index-Column.inline
            strong No.
        #TableData-Column-Header.inline
            for column in columnList.list 
                .TableData-Column-Header.inline
                    span(id='TableData-Header-Column-#{column.id}-Name') 
                        strong #{column.name}
                    span(id='TableData-Header-Column-#{column.id}-Type') (#{column.type})
    p
    #TableData-Body
        #TableData-Index.TableData-Index-Column.inline
        #TableData-Data.inline
