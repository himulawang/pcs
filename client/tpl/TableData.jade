h2.text-info Table Data: #{table.name}
div(id='TableData-#{table.id}-Sheet')
    .row-fluid
        .span1
            table.table.table-condensed
                tr
                    th Column
        .span11
            table.table.table-condensed
                tr
                    for column in columnList.list 
                        th #{column.name} (#{column.type})
    .row-fluid
        .span1
            table#TableData-Index.table.table-condensed.table-striped.table-hover
        .span11
            table#TableData-Data.table.table-condensed.table-striped.table-hover
