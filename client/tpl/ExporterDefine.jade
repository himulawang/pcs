h2.text-info Exporter Define
hr
.row-fluid
    .span8
        .input-prepend
            span.add-on.span4 Id
            input.span6(
                id='ExporterDefine-Exporter-#{exporter.id}-Id'
                type="text"
                value='#{exporter.id}'
                disabled
            )
        .input-prepend
            span.add-on.span4 Name
            input.span6(
                id='ExporterDefine-Exporter-#{exporter.id}-Name'
                type="text"
                value='#{exporter.name}'
                onkeyup='exporterDefineView.onExporterNameChange(#{exporter.id}, this);'
            )
        .input-prepend
            span.add-on.span4 Description
            input.span6(
                id='ExporterDefine-Exporter-#{exporter.id}-Description'
                type="text"
                value='#{exporter.description}'
                onkeyup='exporterDefineView.onExporterDescriptionChange(#{exporter.id}, this);'
            )
        .input-prepend
            span.add-on.span4 Path
            input.span6(
                id='ExporterDefine-Exporter-#{exporter.id}-Path'
                type="text"
                value='#{exporter.path}'
                onkeyup='exporterDefineView.onExporterPathChange(#{exporter.id}, this);'
                placeholder='/home/ila/example.json'
            )
        .input-prepend
            span.add-on.span4 Root Table
            select.span6(
                id='ExporterDefine-Exporter-#{exporter.id}-RootTable'
                class='ExporterDefine-RootTable-Select'
            )
hr                
#ExporterDefine-Levels.row
